{
  "rewrites": [
    {
      "source": "/api/proxy",
      "destination": "/api/proxy.js"
    },
    {
      "source": "/editor",
      "destination": "/editor/index.html"
    },
    {
      "source": "/editor/(.*)",
      "destination": "/editor/$1"
    }
  ],
  "functions": {
    "api/*.js": {
      "runtime": "edge"
    }
  }
}
