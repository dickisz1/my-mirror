{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/proxy",
      "destination": "/api/proxy.js"
    },
    {
      "source": "/editor",
      "destination": "/editor/index.html"
    },
    {
      "source": "/editor/(.*)",
      "destination": "/editor/$1"
    },
    {
      "source": "/app/(.*)",
      "destination": "/app/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/app/index.html"
    }
  ]
}
