{
  "rewrites": [
    { "source": "/editor", "destination": "/editor/index.html" },
    { "source": "/editor/(.*)", "destination": "/editor/$1" },

    { "source": "/app", "destination": "/app/index.html" },
    { "source": "/app/(.*)", "destination": "/app/$1" },

    { "source": "/(.*)", "destination": "/api/index" }
  ]
}
